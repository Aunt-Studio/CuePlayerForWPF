        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CuePlayerForWPF"
        mc:Ignorable="d"
        Title="Theater" Background="{x:Null}" ResizeMode="NoResize" Topmost="True" WindowState="Maximized" WindowStyle="None" Width="1920" Height="1080">
